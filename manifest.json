{

    "manifest_version": 2,
    "name": "No Quote",
    "version": "1.0",
  
    "description": "Adds a unsupported emoji bewtween brackets so nobody can quote your post on the scratch forums",
  
    "icons": {
      "48": "icons/48.png"
      "96": "icons/96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://scratch.mit.edu/discuss/*"],
        "js": ["noquote.js"]
      }
    ]

    "browser_action": {
      "default_icon": "icons/32.png",
      "default_title": "No Quote",
    }
  
  }